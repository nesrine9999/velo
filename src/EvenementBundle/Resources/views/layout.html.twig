<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ title|default('') }} </title>
    <meta name="description" content="Elisyam is a Web App and Admin Dashboard Template built with Bootstrap 4">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Google Fonts -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
    <script>
        WebFont.load({
            google: {"families":["Montserrat:400,500,600,700","Noto+Sans:400,700"]},
            active: function() {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <!-- Favicon -->
    <link id="favicon" rel="shortcut icon" href="{{ asset('favicon.png')}}" type="image/png" />
    <link rel="apple-touch-icon" sizes="194x194" href="{{ asset('apple-touch-icon.png')}}" type="image/png" />
    <!-- Stylesheet -->
    <link rel="stylesheet" href="{{ asset('assets/plugins/bootstrap/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/css/elisyam-1.5.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/css/animate.css') }}">
    <!-- Tweaks for older IEs--><!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
<body id="page-top">
<div class="page db-social">
    <!-- Begin Header -->
    <header class="header">
        <nav class="navbar fixed-top">
            <!-- Begin Search Box-->
            <div class="search-box">
                <button class="dismiss"><i class="ion-close-round"></i></button>
                <form id="searchForm" action="#" role="search">
                    <input type="search" placeholder="Search something ..." class="form-control">
                </form>
            </div>
            <!-- End Search Box-->
            <!-- Begin Topbar -->
            <div class="navbar-holder d-flex align-items-center align-middle justify-content-between">
                <!-- Begin Logo -->
                <div class="navbar-header">
                    <a href="#" class="navbar-brand">
                        <div class="brand-image brand-big">
                            <img src="{{ asset('front/templatetasarim.com/opencart/Bicycle/image/catalog/logo.PNG')}}" alt="logo" class="logo-big" width="130">
                        </div>
                        <div class="brand-image brand-small">
                            <img src="{{ asset('assets/img/logo.png') }}" alt="logo" class="logo-small">
                        </div>
                    </a>

                </div>
                <!-- End Logo -->
                <div class="horizontal-menu">
                    <div class="">
                        <div class="row">
                            <nav class="navbar navbar-light navbar-expand-lg main-menu">
                                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                                    <span></span>
                                    <span></span>
                                    <span></span>
                                </button>
                                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                                    <ul class="navbar-nav mr-auto">
                                        <li><a href="">Accueil</a></li>
                                        <li class="dropdown">
                                            <a class="dropdown-toggle" href="#" id="dashboard" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">evenements</a>
                                            <ul class="dropdown-menu" aria-labelledby="dashboard">
                                                <li><a href="" >Événement sportif</a></li>
                                                <li><a href="" >Festival et défilé</a></li>
                                                <li><a href="" >Tournée promotionnels</a></li>
                                            </ul>
                                        </li>





                                    </ul>
                                </div>
                            </nav>
                        </div>
                    </div>
                </div>
                <!-- Begin Navbar Menu -->

                </li>
                <!-- End Notifications -->
                <!-- User -->
                <li class="nav-item dropdown"><a id="user" rel="nofollow" data-target="#" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="nav-link"><img src="{{ asset('uploads/admin/' ~ app.user.Photo) }}" alt="..." class="avatar rounded-circle"></a>
                    <ul aria-labelledby="user" class="user-size dropdown-menu">
                        <li class="welcome">

                            <img src="{{ asset('uploads/admin/' ~ app.user.Photo) }}" alt="..." class="rounded-circle">
                        </li>
                        <li>
                            <p class="text-center">{{ app.user.nom ~ ' ' ~ app.user.prenom }}</p>
                        </li>


                        <li class="separator"></li>
                        <li><a rel="nofollow" href="{{ path('fos_user_security_logout') }}" class="dropdown-item logout text-center"><i class="ti-power-off"></i></a></li>
                    </ul>
                </li>
                <!-- End User -->

                </ul>
                <!-- End Navbar Menu -->
            </div>
            <!-- End Topbar -->
        </nav>
    </header>
    <!-- End Header -->
    <!-- Begin Page Content -->
    <div class="page-content align-items-stretch">

        <!-- End Left Sidebar -->
        <!-- Begin Content -->
        <div class="">
            <!-- Begin Jumbotron -->
            <div class="jumbotron jumbotron-fluid"></div>
            <!-- End Jumbotron -->
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-xl-11">
                        <!-- Start Head Profile -->
                        <div class="widget head-profile has-shadow">
                            <div class="widget-body pb-0">
                                {% for label, messages in app.flashes %}
                                    {% for message in messages %}
                                        <div class="alert alert-{{ label }}">
                                            {{ message }}
                                        </div>
                                    {% endfor %}
                                {% endfor %}
                                {% block body %} {% endblock %}
                            </div>
                        </div>
                        <!--  block actualite -->
                        <!-- End Head Profile -->

                        <!-- End Row -->
                    </div>
                    <!-- End Col -->
                </div>
                <!-- End Row -->
            </div>
            <!-- End Container -->
            <!-- Begin Page Footer-->
            <!-- End Page Footer -->
            <a href="#" class="go-top"><i class="la la-arrow-up"></i></a>

        </div>
        <!-- End Content -->
    </div>
    <!-- End Page Content -->
</div>
<!-- Begin Vendor Js -->
<script src="{{ asset('assets/plugins/jquery/jquery.min.js') }}"></script>
<script src="{{ asset('assets/plugins/bootstrap/bootstrap.min.js') }}"></script>
<!-- End Vendor Js -->
<!-- Begin Page Vendor Js -->
<script src="{{ asset('assets/plugins/nicescroll/nicescroll.min.js') }}"></script>
<script src="{{ asset('assets/plugins/noty/noty.min.js') }}"></script>
<script src="{{ asset('assets/js/app.js') }}"></script>

{% block js %}{% endblock %}
<!-- Socket io Notification -->


<!-- end notification -->
</body>
</html>